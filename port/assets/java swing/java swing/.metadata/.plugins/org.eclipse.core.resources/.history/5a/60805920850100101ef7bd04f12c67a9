package swingpack;
//Define an interface with a default method
interface MyInterface {
 void showMessage(); // Abstract method

 // Default method with implementation
 default void displayDefault() {
     System.out.println("This is a default method inside the interface.");
 }
}

//MyClass implementing MyInterface
public class MyClass implements MyInterface {
 @Override
 public void showMessage() {
     System.out.println("Hello! This is an implemented method.");
 }

 // Main method
 public static void main(String[] args) {
     MyClass obj = new MyClass();
     obj.showMessage();       // Calls the overridden method
     obj.displayDefault();    // Calls the default method from the interface
 }
}


