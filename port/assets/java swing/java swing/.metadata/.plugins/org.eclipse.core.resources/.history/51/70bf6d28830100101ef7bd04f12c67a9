package swingpack;
//Define an interface with a default method
interface MyInterface {
 void abstractMethod(); // Abstract method to be implemented

 // Default method with implementation
 default void defaultMethod() {
     System.out.println("This is a default method in the interface.");
 }
}

//Implement the interface in a class
class MyClass implements MyInterface {
 @Override
 public void abstractMethod() {
     System.out.println("Abstract method implemented in MyClass.");
 }

 // Optional: Overriding the default method
 @Override
 public void defaultMethod() {
     System.out.println("Overriding the default method in MyClass.");
 }
}

//Main class to execute the program
public class Main {
 public static void main(String[] args) {
     MyClass obj = new MyClass();
     obj.abstractMethod(); // Calls the implemented abstract method
     obj.defaultMethod(); // Calls the overridden default method
 }
}

